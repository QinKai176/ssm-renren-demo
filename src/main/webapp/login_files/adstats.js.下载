function sendStats(o){var e="log_"+(new Date).getTime(),n=window[e]=new Image;n.onload=n.onerror=function(){window[e]=null},n.src=o,n=null}function goPAGE(){window.location.href.split("#")[0];return navigator.userAgent.match(/(phone|pad|pod|iPhone|iPod|ios|Android|Mobile|BlackBerry|IEMobile|MQQBrowser|JUC|Fennec|wOSBrowser|BrowserNG|WebOS|Symbian|Windows Phone)/i)?"wap":"pc"}function scrollReq(o,e){var n=goPAGE();if(o.offsetTop>=document.body.scrollTop&&o.offsetTop<document.body.scrollTop+document.documentElement.clientHeight){if(!o.dataset.pv)return;sendStats('http://dj.renren.com/adstats?j={"from":"'+window.location.hostname+'","dev":"'+n+'","page":"'+encodeURIComponent(window.location.href)+'","adId":"'+o.dataset.pv+'"}'),o.dataset.pv="",e&&e()}}